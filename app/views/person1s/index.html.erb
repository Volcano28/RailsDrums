<h1>Listing person1s</h1>

<table>
  <thead>
    <tr>
      <th>Snaredrum carts</th>
      <th>Bassdrum carts</th>
      <th>Hightomtom carts</th>
      <th>Floortomtom carts</th>
      <th>Hihat carts</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @person1s.each do |person1| %>
      <tr>
        <td><%= person1.snaredrum_carts %></td>
        <td><%= person1.bassdrum_carts %></td>
        <td><%= person1.hightomtom_carts %></td>
        <td><%= person1.floortomtom_carts %></td>
        <td><%= person1.hihat_carts %></td>
        <td><%= link_to 'Show', person1 %></td>
        <td><%= link_to 'Edit', edit_person1_path(person1) %></td>
        <td><%= link_to 'Destroy', person1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person1', new_person1_path %>
